{
  "proc command": {
    "prefix": "proc",
    "description": "Specify a tcl proc",
    "body": ["proc ${1:name} {${2:vars}} {", "\t$0", "}"]
  },
  "if command": {
    "prefix": "if",
    "description": "An if statement",
    "body": ["if {${1:expression}} {", "\t$0", "}"]
  },
  "for command": {
    "prefix": "for",
    "description": "A for loop",
    "body": ["for {${1:set i 0}} {${2:\\$i < 1}} {${3:incr i}} {", "\t$0", "}"]
  }
}
